$('#cogs').html("$<%= number_with_precision(@value, :precision => 2, :delimiter => ',') %>")
$('#orders').html("<%= number_with_precision(@orders, :precision => 0, :delimiter => ',') %>")
$('#averagecogs').html("$<%= number_with_precision((@orders > 0 ? @value/@orders : 0), :precision => 2, :delimiter => ',') %>")

