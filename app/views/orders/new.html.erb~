<%= form_for :order,:url=>{:controller=>"orders", :action=>"create"}, do |f| %>
  <%= f.file_field :file %><br />
	<%= f.label :origin %>
	<%= f.radio_button :origin, "Amazon US", checked: true %>Amazon US - last order:<%= Order.find_all_by_origin("Amazon US").last.date %><br>
	<%= f.radio_button :origin, "Amazon Canada" %>Amazon Canada - last order:<%= Order.find_all_by_origin("Amazon Canada").sort_by(&:date).last.date %><br>
	<%= f.radio_button :origin, "Website" %>Rubberbanditz Website - last order:<%= Order.find_all_by_origin("Website").sort_by(&:date).last.date %><br>
  <%= f.submit "Load Data" %>
<% end %>
