<%= form_for @order do |f| %>
  <%= f.label :order_number %>
  <%= f.text_field :order_number, value: Order.count+1 %>
  <%= f.label :date %>
  <%= f.date_select :date %>
  <%= f.label :offering %>
  <%= f.collection_select :offering_id, Offering.all, :id, :name %>
  <%= f.label :quantity %>
  <%= f.text_field :quantity %>
  <%= f.label :origin %>
  <%= f.text_field :origin, value: "Phone/Email" %>
  <%= f.submit "Load Data" %>
<% end %>
