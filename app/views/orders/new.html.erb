<%= form_for :order do |f| %>
  <%= f.file_field :file %><br />
	<%= f.label :type %>
	<%= f.radio_button :type, "Amazon US", checked: true %>Amazon US - last order:<%= Order.find_all_by_origin("Amazon US").sort_by(&:date).last.date %><br>
	<%= f.radio_button :type, "Amazon Canada" %>Amazon Canada - last order:<%= Order.find_all_by_origin("Amazon Canada").sort_by(&:date).last.date %><br>
	<%= f.radio_button :type, "Rubberbanditz Website" %>Rubberbanditz Website - last order:<%= Order.find_all_by_origin("Website").sort_by(&:date).last.date %><br>
  <%= f.submit "Load Data" %>
<% end %>
